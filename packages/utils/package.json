{
  "name": "@react-ssr-frame/utils",
  "version": "1.0.0",
  "description": "utils for ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "cjs",
    "esm"
  ],
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json \" \" tsc -p ./tsconfig.esm.json\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DXM666/react-ssr-frame"
  },
  "keywords": [
    "cli",
    "ssr",
    "react"
  ],
  "author": "DXM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DXM666/react-ssr-frame/issues"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "homepage": "https://github.com/DXM666/react-ssr-frame#readme",
  "dependencies": {
    "@react-ssr-frame/types": "^1.0.3",
    "concurrently": "^8.2.2"
  }
}
